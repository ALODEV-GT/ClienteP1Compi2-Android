<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".teclado">

    <Button
        android:id="@+id/guardarBtn"
        android:layout_width="170dp"
        android:layout_height="40dp"
        android:background="@color/success"
        android:text="Guardar"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.502"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/laSBtn"
        app:layout_constraintVertical_bias="0.733" />

    <Chronometer
        android:id="@+id/chronometer"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.004" />

    <RadioGroup
        android:id="@+id/octavasRg"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.04"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView3"
        app:layout_constraintVertical_bias="0.02">


        <RadioButton
            android:id="@+id/octava1Rb"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="1"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0"
            app:layout_constraintStart_toStartOf="parent"
            tools:layout_editor_absoluteY="140dp" />

        <RadioButton
            android:id="@+id/octava2Rb"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="2"
            app:layout_constraintEnd_toStartOf="@+id/option1"
            app:layout_constraintHorizontal_bias="0"
            app:layout_constraintStart_toEndOf="@+id/option3"
            tools:layout_editor_absoluteY="140dp" />

        <RadioButton
            android:id="@+id/octava3Rb"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="3"
            app:layout_constraintHorizontal_bias="0"
            tools:layout_editor_absoluteX="153dp"
            tools:layout_editor_absoluteY="140dp" />

        <RadioButton
            android:id="@+id/octava4Rb"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="4"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.082"
            app:layout_constraintStart_toStartOf="parent"
            tools:layout_editor_absoluteY="140dp" />

        <RadioButton
            android:id="@+id/octava5Rb"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="5"
            app:layout_constraintEnd_toStartOf="@+id/option1"
            app:layout_constraintHorizontal_bias="0.481"
            app:layout_constraintStart_toEndOf="@+id/option3"
            tools:layout_editor_absoluteY="140dp" />

        <RadioButton
            android:id="@+id/octava6Rb"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="6"
            tools:layout_editor_absoluteX="153dp"
            tools:layout_editor_absoluteY="140dp" />

        <RadioButton
            android:id="@+id/octava7Rb"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="7"
            app:layout_constraintEnd_toStartOf="@+id/option1"
            app:layout_constraintHorizontal_bias="0.481"
            app:layout_constraintStart_toEndOf="@+id/option3"
            tools:layout_editor_absoluteY="140dp" />

        <RadioButton
            android:id="@+id/octava8Rb"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="8"
            tools:layout_editor_absoluteX="153dp"
            tools:layout_editor_absoluteY="140dp" />

    </RadioGroup>

    <TextView
        android:id="@+id/tiempoTv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="0"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.08"
        app:layout_constraintStart_toEndOf="@+id/textView4"
        app:layout_constraintTop_toBottomOf="@+id/canalTv"
        app:layout_constraintVertical_bias="0.059" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="OCTAVA"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.05"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/canalTv"
        app:layout_constraintVertical_bias="0.06" />

    <TextView
        android:id="@+id/nombreTv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Nombre:"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.05"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.045" />

    <TextView
        android:id="@+id/canalTv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Canal:"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.05"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/nombreTv"
        app:layout_constraintVertical_bias="0.04" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="NOTAS"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.08"
        app:layout_constraintStart_toEndOf="@+id/textView3"
        app:layout_constraintTop_toBottomOf="@+id/canalTv"
        app:layout_constraintVertical_bias="0.06" />

    <Button
        android:id="@+id/laBtn"
        android:layout_width="85dp"
        android:layout_height="85dp"
        android:text="La"
        android:background="@color/success"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.06"
        app:layout_constraintStart_toEndOf="@+id/octavasRg"
        app:layout_constraintTop_toBottomOf="@+id/faSBtn"
        app:layout_constraintVertical_bias="0.04" />

    <Button
        android:id="@+id/doBtn"
        android:layout_width="85dp"
        android:layout_height="85dp"
        android:background="@color/success"
        android:text="Do"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.06"
        app:layout_constraintStart_toEndOf="@+id/octavasRg"
        app:layout_constraintTop_toBottomOf="@+id/textView3"
        app:layout_constraintVertical_bias="0.029" />

    <Button
        android:id="@+id/reBtn"
        android:layout_width="85dp"
        android:layout_height="85dp"
        android:background="@color/success"
        android:text="Re"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.17"
        app:layout_constraintStart_toEndOf="@+id/doSBtn"
        app:layout_constraintTop_toBottomOf="@+id/textView3"
        app:layout_constraintVertical_bias="0.029" />

    <Button
        android:id="@+id/reSBtn"
        android:layout_width="85dp"
        android:layout_height="85dp"
        android:text="Re#"
        android:background="@color/success"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.06"
        app:layout_constraintStart_toEndOf="@+id/octavasRg"
        app:layout_constraintTop_toBottomOf="@+id/doBtn"
        app:layout_constraintVertical_bias="0.02" />

    <Button
        android:id="@+id/miBtn"
        android:layout_width="85dp"
        android:layout_height="85dp"
        android:text="Mi"
        android:background="@color/success"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.06"
        app:layout_constraintStart_toEndOf="@+id/reSBtn"
        app:layout_constraintTop_toBottomOf="@+id/doSBtn"
        app:layout_constraintVertical_bias="0.02" />

    <Button
        android:id="@+id/faBtn"
        android:layout_width="85dp"
        android:layout_height="85dp"
        android:text="Fa"
        android:background="@color/success"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.17"
        app:layout_constraintStart_toEndOf="@+id/miBtn"
        app:layout_constraintTop_toBottomOf="@+id/reBtn"
        app:layout_constraintVertical_bias="0.02" />

    <Button
        android:id="@+id/faSBtn"
        android:layout_width="85dp"
        android:layout_height="85dp"
        android:text="Fa#"
        android:background="@color/success"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.06"
        app:layout_constraintStart_toEndOf="@+id/octavasRg"
        app:layout_constraintTop_toBottomOf="@+id/reSBtn"
        app:layout_constraintVertical_bias="0.02" />

    <Button
        android:id="@+id/solBtn"
        android:layout_width="85dp"
        android:layout_height="85dp"
        android:text="Sol"
        android:background="@color/success"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.06"
        app:layout_constraintStart_toEndOf="@+id/faSBtn"
        app:layout_constraintTop_toBottomOf="@+id/miBtn"
        app:layout_constraintVertical_bias="0.02" />

    <Button
        android:id="@+id/solSBtn"
        android:layout_width="85dp"
        android:layout_height="85dp"
        android:text="Sol#"
        android:background="@color/success"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.17"
        app:layout_constraintStart_toEndOf="@+id/solBtn"
        app:layout_constraintTop_toBottomOf="@+id/faBtn"
        app:layout_constraintVertical_bias="0.02" />

    <Button
        android:id="@+id/siBtn"
        android:layout_width="85dp"
        android:layout_height="85dp"
        android:text="Si"
        android:background="@color/success"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.17"
        app:layout_constraintStart_toEndOf="@+id/laSBtn"
        app:layout_constraintTop_toBottomOf="@+id/solSBtn"
        app:layout_constraintVertical_bias="0.04" />

    <Button
        android:id="@+id/doSBtn"
        android:layout_width="85dp"
        android:layout_height="85dp"
        android:background="@color/success"
        android:text="Do#"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.061"
        app:layout_constraintStart_toEndOf="@+id/doBtn"
        app:layout_constraintTop_toBottomOf="@+id/textView3"
        app:layout_constraintVertical_bias="0.029" />

    <Button
        android:id="@+id/laSBtn"
        android:layout_width="85dp"
        android:layout_height="85dp"
        android:text="La#"
        android:background="@color/success"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.06"
        app:layout_constraintStart_toEndOf="@+id/laBtn"
        app:layout_constraintTop_toBottomOf="@+id/solBtn"
        app:layout_constraintVertical_bias="0.04" />

    <EditText
        android:id="@+id/nombreEt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="text"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.087"
        app:layout_constraintStart_toEndOf="@+id/nombreTv"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.032" />

    <EditText
        android:id="@+id/canalEd"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="number"
        android:text="0"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.047"
        app:layout_constraintStart_toEndOf="@+id/canalTv"
        app:layout_constraintTop_toBottomOf="@+id/nombreTv"
        app:layout_constraintVertical_bias="0.032" />

    <Button
        android:id="@+id/silencioBtn"
        android:layout_width="268dp"
        android:layout_height="55dp"
        android:background="@color/success"
        android:text="Silencio"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.3"
        app:layout_constraintStart_toEndOf="@+id/octavasRg"
        app:layout_constraintTop_toBottomOf="@+id/laSBtn"
        app:layout_constraintVertical_bias="0.04" />


</androidx.constraintlayout.widget.ConstraintLayout>